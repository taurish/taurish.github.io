def generate_leaderboard(leaders):
    return "<li><mark>"+leaders[0][0]+"</mark><small>"+str(leaders[0][1])+"</small></li><li><mark>"+leaders[1][0]+"</mark><small>"+str(leaders[1][1])+"</small></li>"+"<li><mark>"+leaders[2][0]+"</mark><small>"+str(leaders[2][1])+"</small></li><li><mark>"+leaders[3][0]+"</mark><small>"+str(leaders[3][1])+"</small></li><li><mark>"+leaders[4][0]+"</mark><small>"+str(leaders[4][1])+"</small></li>"
